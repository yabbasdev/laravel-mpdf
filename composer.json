{
    "name": "yabbasdev/laravel-mpdf-arabic",
    "description": "Laravel Mpdf: Using Mpdf in Laravel to generate Pdfs.",
    "keywords": ["mpdf", "pdf", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^5.6|^7.0|^8.0|^8.1|^8.2|^8.3",
        "abdallahalyamni/mpdf-arabic": "dev-development"
    },
    "autoload": {
        "psr-4": {
            "AbdallahAlyamni\\LaravelMpdfArabic\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                 "AbdallahAlyamni\\LaravelMpdfArabic\\LaravelMpdfServiceProvider"
            ],
            "aliases": {
                "PDF": "AbdallahAlyamni\\LaravelMpdfArabic\\Facades\\LaravelMpdf"
            }
        }
    }
}
